    "query": "SELECT\n        c.id, c.id_parent, c.id_request, c.content,\n        (u.image, u.name, u.username) as \"user!: User\"\n        FROM Comment c\n        INNER JOIN Account u\n        ON c.owner = u.username\n        WHERE id_request = $1\n        ORDER BY id"
  },
